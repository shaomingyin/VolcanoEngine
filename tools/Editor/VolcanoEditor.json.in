{
    "Id": "volcanoeditor",
	"Name": "Volcano Editor",
	"Version": "${CMAKE_PROJECT_VERSION}_0",
	"Vendor": "Shaomy",
	"VendorId": "shaomy",
	"Mimetypes": [
	    "<?xml version='1.0' encoding='UTF-8'?>",
		"<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>",
		"    <mime-type type='${VOLCANO_EDITOR_MIMETYPE_PROJECT_FILE}'>",
		"        <comment>Volcano Project File</comment>",
		"        <sub-class-of type='application/json'/>",
		"        <glob pattern='*.volcano-project'/>",
		"    </mime-type>",
		"    <mime-type type='${VOLCANO_EDITOR_MIMETYPE_SCENE_FILE}'>",
		"        <sub-class-of type='application/json'/>",
		"        <comment>Volcano Scene File</comment>",
		"        <glob pattern='*.volcano-scene'/>",
		"    </mime-type>",
		"</mime-info>"
	],
	${IDE_PLUGIN_DEPENDENCIES}
}
